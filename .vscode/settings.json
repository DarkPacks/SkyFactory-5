{
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "eslint.codeActionsOnSave.rules": ["*"],
  "eslint.lintTask.enable": true,
  "eslint.rules.customizations": [
    {
      "rule": "*",
      "severity": "default"
    },
    {
      "rule": "editorconfig/*",
      "severity": "off"
    },
    {
      "rule": "no-multiple-empty-lines",
      "severity": "off"
    },
    {
      "rule": "toml/spaced-comment",
      "severity": "off"
    }
  ],
  "eslint.probe": [
    "plaintext",
    "javascript",
    "javascriptreact",
    "json",
    "json5",
    "jsonc",
    "toml",
    "typescript",
    "yaml",
    "zencode"
  ],
  "eslint.validate": [
    "plaintext",
    "javascript",
    "javascriptreact",
    "json",
    "json5",
    "jsonc",
    "toml",
    "typescript",
    "yaml",
    "zencode"
  ],
  "files.associations": {
    "*.mcmeta": "json"
  },
  "files.exclude": {
    ".eslintcache": true,
    ".git": true,
    "node_modules": true,
    ".tmp": true,
    ".husky": true
  },
  "prettier.prettierPath": "./node_modules/prettier",
  "prettier.enable": true,
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/.tmp": true,
    "./minecraft": true
  },
  "BC-MC.Education.Enable": false,
  "BC-MC.Plugin.CodeLens": false,
  "BC-MC.Diagnostics.Tags": false,
  "BC-MC.Diagnostics.Objectives": false,
  "BC-MC.Diagnostics.Mcfunctions": false,
  "BC-MC.Diagnostics.Lang": false,
  "BC-MC.Diagnostics.Json": false,
  "BC-MC.Diagnostics.Enable": false,
  "json.schemas": [
    {
      "fileMatch": ["/mc-package.json"],
      "url": "./schemas/mc-package.json"
    },
    {
      "fileMatch": ["/src/minecraft/config/incontrol/loot.json"],
      "url": "./schemas/minecraft/incontrol/loot.json"
    },
    {
      "fileMatch": ["/src/minecraft/config/incontrol/spawn.json"],
      "url": "./schemas/minecraft/incontrol/spawn.json"
    },
    {
      "fileMatch": ["/src/minecraft/config/incontrol/spawner.json"],
      "url": "./schemas/minecraft/incontrol/spawner.json"
    },
    {
      "fileMatch": ["/src/minecraft/thingpacks/*/things/*/item/**/*.json"],
      "url": "./schemas/minecraft/jsonthings/item.json"
    },
    {
      "fileMatch": ["/src/minecraft/thingpacks/*/things/*/block/**/*.json"],
      "url": "./schemas/minecraft/jsonthings/block.json"
    },
    {
      "fileMatch": [
        "/src/minecraft/thingpacks/*/things/*/block_set_type/**/*.json"
      ],
      "url": "./schemas/minecraft/jsonthings/block-set-type.json"
    },
    {
      "fileMatch": [
        "/src/minecraft/thingpacks/*/things/*/creative_mode_tab/**/*.json"
      ],
      "url": "./schemas/minecraft/jsonthings/creative-mode-tab.json"
    },
    {
      "fileMatch": ["/src/minecraft/thingpacks/*/things/*/food/**/*.json"],
      "url": "./schemas/minecraft/jsonthings/food.json"
    },
    {
      "fileMatch": ["/lang/*.json"],
      "url": "./.vscode/probe.lang-schema.json"
    },
    {
      "fileMatch": ["/probe/docs/*.json"],
      "url": "./.vscode/probe.doc-schema.json"
    }
  ],
  "cSpell.enabledLanguageIds": [
    "javascript",
    "json",
    "json5",
    "jsonc",
    "markdown",
    "mcfunction",
